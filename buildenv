export ZOPEN_GIT_URL="https://github.com/bminor/bash.git"
export ZOPEN_GIT_DEPS="git make gzip tar"
export ZOPEN_TARBALL_URL="https://ftp.gnu.org/gnu/bash/bash-5.2.tar.gz"
export ZOPEN_TARBALL_DEPS="curl gzip tar make ncurses zoslib"
export ZOPEN_TYPE="TARBALL"
export ZOPEN_EXTRA_CPPFLAGS="-DNO_MAIN_ENV_ARG=1 -DNSIG=42"

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  failures=$(/bin/find tests/ -name "*.output"  ! -size 0 | wc -l)
  total=$(/bin/find tests/ -name "*.output" | wc -l)

  # Echo the following information to guage build health
  echo "actualFailures:$failures"
  echo "totalTests:$total"
  echo "expectedFailures:30"
}
